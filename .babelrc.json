{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": {
          "version": "3",
          "proposals": true
        },
        "useBuiltIns": "usage",
        "loose": true
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
  ]
}
